
@{
    ViewBag.Title = "OrderRequest";
    Layout = "~/Views/Shared/_LayoutWarehouseManager.cshtml";
}

<h2>OrderRequest</h2>
@model LogiManage.ViewModels.OrderRequestViewModel
<div>
    <h3> Add Order Request</h3>
    @using (Html.BeginForm("AddOrderRequest", "Warehouse", FormMethod.Post))
    {
        @Html.HiddenFor(m => m.OrderRequestID)

        @Html.LabelFor(m => m.OrderRequestStatus)
        @Html.TextBoxFor(m => m.OrderRequestStatus)

        @Html.LabelFor(m => m.OrderRequestDate)
        @Html.TextBoxFor(m => m.OrderRequestDate)

        @Html.LabelFor(m => m.ProductID)
        @Html.TextBoxFor(m => m.ProductID)

        @Html.LabelFor(m => m.WarehouseID)
        @Html.TextBoxFor(m => m.WarehouseID)


        @Html.LabelFor(m => m.RequestQuantity)
        @Html.TextBoxFor(m => m.RequestQuantity)

        <button type="submit" class="btn btn-outline-success">Add </button>
    }
</div>
